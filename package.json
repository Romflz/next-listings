{
  "name": "next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "db:migrate": "dotenv -e .env.local -- npx prisma migrate dev",
    "db:generate": "dotenv -e .env.local -- npx prisma generate",
    "db:studio": "dotenv -e .env.local -- npx prisma studio",
    "db:migrate:prod": "dotenv -e .env.production -- npx prisma migrate deploy",
    "db:generate:prod": "dotenv -e .env.production -- npx prisma generate",
    "build:prod": "npm run db:generate:prod && npm run db:migrate:prod && next build --turbopack"
  },
  "dependencies": {
    "@next/env": "^15.5.2",
    "@prisma/client": "^6.16.1",
    "@prisma/extension-accelerate": "^2.0.2",
    "firebase": "^12.2.1",
    "firebase-admin": "^13.5.0",
    "lucide-react": "^0.543.0",
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "swiper": "^11.2.10"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "eslint-plugin-boundaries": "^5.0.1",
    "prisma": "^6.16.1",
    "tailwindcss": "^4",
    "tsx": "^4.20.5",
    "typescript": "^5"
  }
}
